<%= simple_form_for(@link) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :url %>
    <%= f.association :band_profile %>
    <%= f.association :member %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
