<%= simple_form_for(@gig) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :date %>
    <%= f.input :start_time %>
    <%= f.input :finish_time %>
    <%= f.input :location %>
    <%= f.input :street_address %>
    <%= f.input :suburb %>
    <%= f.input :tickets_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
